      <div class="search-container">
        <div class="searchbar">
          <nav class="navbar bg-body-tertiary">
            <div class="container-fluid">
              <form class="d-flex w-100" role="search" method="post" action="/search">
                <input class="form-control me-2 w-100" type="search" placeholder="Search" aria-label="Search" name="searchedQuery">
                <button class="btn btn-outline-success" type="submit">Search</button>
              </form>
            </div>
          </nav>
        </div>

        <!-- Logo -->
        <div class="brand">
          <img src="./images/wizard.png" alt="image : wizard" id="logo">
          <span class="seek text-primary">Seek</span><span class="sphere text-danger">Sphere</span>
        </div>
      </div>
      <!-- Logo ends here -->
    
      <!-- Footer -->
      <div class="footer-container">
        <div class="social-icons">
          <ul>
            <li><a href="https://www.linkedin.com/in/naved-uddin-800241195/" target="_blank"><i class="fa-brands fa-linkedin icons"></i></a></li>
            <li><a href="https://github.com/Naved-Uddin9950/" target="_blank"><i class="fa-brands fa-github icons"></i></a></li>
            <li><a href="https://www.instagram.com/_n_a_v_e_d_99/" target="_blank"><i class="fa-brands fa-instagram icons"></i></a></li>
